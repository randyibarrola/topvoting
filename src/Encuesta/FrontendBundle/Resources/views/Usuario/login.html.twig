
Pagina de Login

<form class="formulario" action="{{ path('frontend_login_check') }}" method="post">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
    <tr>
        <td width="32%" height="32" align="left" class="tit_negro16">Iniciar sesi&oacute;n</td>
    </tr>
    <tr>
        <td height="30" align="left" valign="bottom" class="txt_negro12_bold">Nombre de Usuario</td>
    </tr>
    <tr>
        <td height="30" align="left"><input name="_username" type="text" class="txt_negro12 validate[required]" id="textfield" size="55" /></td>
    </tr>
    <tr>
        <td height="30" align="left" valign="bottom" class="txt_negro12_bold">Contrase&ntilde;a</td>
    </tr>
    <tr>
        <td height="30" align="left"><input name="_password" type="password" class="txt_negro12 validate[required]" id="textfield2" size="55" /></td>
    </tr>
    <tr>
        <td height="30" align="left"><input name="" type="submit" value="Entrar" /></td>
    </tr> 
    {% if error %}
    <tr>
        <td>{{ error.message }}</td>
    </tr>
    {% endif %}
</table>
        