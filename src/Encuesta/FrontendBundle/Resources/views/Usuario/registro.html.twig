{% else %}

<form class="formulario" action="{{ path('frontend_registro') }}" method="post">

    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
            <td width="32%" height="32" align="left" class="tit_negro16">Registro</td>
        </tr>
        <tr>
            <td height="30" align="left" valign="bottom" class="txt_negro12_bold">Nombre de Usuario</td>
        </tr>
        <tr>
            <td height="30" align="left">{{ form_widget(form.username) }}</td>
        </tr>
        <tr>
            <td height="30" align="left" valign="bottom" class="txt_negro12_bold">Email</td>
        </tr>
        <tr>
            <td height="30" align="left">{{ form_widget(form.email) }}</td>
        </tr>

        <tr>
            <td height="30" align="left" valign="bottom" class="txt_negro12_bold">Contrase&ntilde;a</td>
        </tr>
        <tr>
            <td height="30" align="left">{{ form_widget(form.password) }}</td>
        </tr>
        <tr>
            <td height="30" align="left"><input name="" type="submit" value="Entrar" /></td>
        </tr> 
    </table>

    {{ form_errors(form) }}
    {{ form_widget(form._token) }}        
</form>

{% endif %}